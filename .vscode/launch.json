{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch test function",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceRoot}",
            "args": [
                "-test.run",
                "MyTestFunction"
            ]
        },
        {
            "name": "Ravi Debugger",
            "type": "lua",
            "request": "launch",
            "program": "${workspaceRoot}/main.lua",
            "stopOnEntry": true,
            "cwd": "${workspaceRoot}",
            "LUA_PATH": "${workspaceRoot}/?.lua",
            "LUA_CPATH": "${workspaceRoot}/?.dll"
        }
    ]
}